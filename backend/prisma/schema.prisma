generator client {
  provider = "prisma-client-js"
  moduleFormat = "esm"
  generatedFileExtension = "js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  passwordHash String 
  role Role
}

enum Role {
  CANDIDATE 
  EMPLOYEE
  ADMIN
}
