generator client {
  provider               = "prisma-client-js"
  output                 = "./generated/prisma" // Your custom output path
  moduleFormat           = "esm" // Explicitly set to ESM
  generatedFileExtension = "js" // Generates .js files
}

datasource db {
  provider = "postgresql"
}

model User {
  id           Int    @id @default(autoincrement())
  email        String @unique
  passwordHash String
}
